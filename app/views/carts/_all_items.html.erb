    <div class="col-main">
      <div class="cart wow bounceInUp animated">
        <div class="page-title">
          <h2>Shopping Cart</h2>
        </div>
        
        <div class="table-responsive">
            <input type="hidden" value="Vwww7itR3zQFe86m" name="form_key">
            <fieldset>
              <table class="data-table cart-table" id="shopping-cart-table">
                <colgroup>
                <col width="1">
                <col>
                <col width="1">
                <col width="1">
                <col width="1">
                <col width="1">
                <col width="1">
                </colgroup>
                <thead>
                  <tr class="first last">
                    <th rowspan="1">&nbsp;</th>
                    <th rowspan="1"><span class="nobr">Product Name</span></th>
                    <!-- <th rowspan="1"></th> -->
                    <th colspan="1" class="a-center"><span class="nobr">Unit Price</span></th>
                    <th class="a-center" rowspan="1">Qty</th>
                    <th colspan="1" class="a-center">Subtotal</th>
                    <th class="a-center" rowspan="1">&nbsp;</th>
                  </tr>
                </thead>
                <tfoot>
                  <tr class="first last">
                    <td class="a-right last" colspan="50">
                    <%= link_to root_path do %>
                      <button onclick="setLocation('<%= root_path %>')" class="button btn-continue" title="Continue Shopping" type="button">
                        <span><span>Continue Shopping</span></span>
                      </button>
                      <% end %>


                <%= link_to empty_carts_carts_path, method: :delete do %>
                      <button id="empty_cart_button" class="button btn-empty" title="Clear Cart" value="empty_cart" name="update_cart_action" type="submit">
                        <span><span>Clear Cart</span></span>
                      </button>
                <% end %>

                    </td>
                  </tr>
                </tfoot>

                <tbody>
              <% @user_products.each do |line_item| %>
                <%= form_tag pay_line_item_orders_path(line_item.id), :method => 'GET', :id => "submit_cart_#{line_item.id}" do %>
                    <tr class="first odd" id="line-item-<%= line_item.id %>">

                      <td class="image">
                      <!-- <a class="product-image" title="Sample Product" href="#/women-s-crepe-printed-black/"> -->
                      <%= image_tag(line_item.product.images.first.image.url(:original), :width => '75', :height => '75', :id => "image_#{line_item.id}", :style => 'cursor: pointer;') %>
                      <!-- </a> -->
                      </td>
                      
                      <td><h2 class="product-name">
                       <a>
                      <%= line_item.product.title %>
                      </a> </h2></td>
                      
                      <!-- <td class="a-center"><a title="Edit item parameters" class="edit-bnt" href="#configure/id/15945/"></a></td> -->
                      
                      <td class="a-right">
                        <span class="cart-price">
                          <span class="price">$</span>
                          <span class="price" id="cart-unit-price-<%= line_item.id %>">
                            <%= number_to_currency(line_item.unit_price, unit: '') %>
                          </span> 
                        </span>
                      </td>
                      
                      <td class="a-center movewishlist">
                        <%= number_field_tag :qty, line_item.quantity, maxlength: "12", id: "cart-qty", class: "input-text qty", title: "Qty", size: '5', min: '1', max: '99', data: { line_item_id: line_item.id } %>
                      </td>

                      <td class="a-right movewishlist" id="line-item-sub-total">
                        <span class="cart-price">
                          <span class="price" id="lineitem-subtotal">
                            <%= number_to_currency(line_item.unit_price.to_f * line_item.quantity.to_i) %>
                          </span>
                        </span>
                      </td>

                      <td class="a-center last">
                      <%= link_to "<span><span>Remove item</span></span>".html_safe, cart_path(line_item), :method => :delete, :confirm => "Are You Sure", :class => "button remove-item" %>
                      </td>
                    </tr>
                  <% end %>
                <% end %>
                </tbody>
              </table>
            </fieldset>
        </div>
        <!-- BEGIN CART COLLATERALS -->
        <div class="cart-collaterals row">
          
         
        <div class="totals col-sm-4">
          <h3>Shopping Cart Total</h3>
          <div class="inner">
            <table class="table shopping-cart-table-total" id="shopping-cart-totals-table">
              <colgroup>
              <col>
              <col width="1">
              </colgroup>
              <tfoot>
                <tr>
                  <td colspan="1" class="a-left" style=""><strong>Grand Total</strong></td>
                  <td class="a-right" style=""><strong><span class="price"><%= number_to_currency(@cart.total_price) %></span></strong></td>
                </tr>
              </tfoot>
              <!-- <tbody>
                <tr>
                  <td colspan="1" class="a-left" style=""> Subtotal </td>
                  <td class="a-right" style=""><span class="price">$77.38</span></td>
                </tr>
              </tbody> -->
            </table>
            <ul class="checkout">
              <li>
                <button onclick="window.location='http://demo.magikthemes.com/index.php/eclipseblue/checkout/onepage/';" class="button btn-proceed-checkout" title="Proceed to Checkout" type="button"><span>Proceed to Checkout</span></button>
              </li>
              <br>
  
              <br>
            </ul>
          </div>
          <!--inner--> 
          
        </div>
      </div>
      
      <!--cart-collaterals--> 
      
    </div>
    
  </div>

<!-- End Footer -->

